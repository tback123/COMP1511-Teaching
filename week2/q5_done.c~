// Tom Killingback
// Test leap years

#include <stdio.h>

int main(void) {

    int year = 2020;
    
    if (year % 4 == 0) {
    
        if (year % 100 != 0) {
        
            if (year % 4 == 0 ) {
                
                printf("%d is a leap year\n", year);
                
            }
        
        }
    
    }

}
